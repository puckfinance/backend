{
  "name": "srve-backend",
  "version": "1.0.0",
  "main": "index.js",
  "description": "nodejs + typescript",
  "repository": {},
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/server.js",
    "stage": "dotenv -e .env -- ts-node-dev --respawn --transpile-only --exit-child ./src/server.ts",
    "dev": "dotenv -e .env.local -- ts-node-dev --respawn --transpile-only --exit-child ./src/server.ts",
    "prod:run": "tsc  && npm prune --production && node ./dist/server.js",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "prisma:seed": "dotenv -e .env.local -- npx prisma db seed",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:reset": "dotenv -e .env.local -- prisma migrate reset",
    "prisma:migrate:prod": "dotenv -e .env.local -- prisma migrate deploy",
    "test": "dotenv -e .env.local -- jest",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:db:push": "npx prisma db push",
    "build": "tsc",
    "clean": "rm -rf dist node_modules yarn.lock package.lock.json",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "axios": "^1.3.1",
    "bcrypt": "^5.1.0",
    "binance-api-node": "^0.12.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "express": "^4.18.2",
    "express-json-error-handler": "^5.0.4",
    "express-rate-limit": "^6.7.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "minio": "^7.0.32",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.10",
    "stripe": "^14.1.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.38",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/minio": "^7.0.15",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-github2": "^1.2.5",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/passport-oauth2-refresh": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express-list-endpoints": "^6.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.4.2",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
